package modelo;

public abstract class ServicioExtraMonitoreo 
{
	private boolean monitoreoCamara;
	private boolean botonAntipanico;
	private boolean movilAcompanamiento;
	
	public ServicioExtraMonitoreo(boolean monitoreoCamara, boolean botonAntipanico, boolean movilAcompanamiento) 
	{
		this.monitoreoCamara = monitoreoCamara;
		this.botonAntipanico = botonAntipanico;
		this.movilAcompanamiento = movilAcompanamiento;
	}
	
	public abstract float valorExtra();
}
